                        "(.+)@", 
                        "@(.+)"
                    ], 
                    "preserve_original": 1, 
                    "type": "pattern_capture"
                }, 
                "filter_stemmer": {
                    "language": "English", 
                    "type": "porter_stem"
                }, 
                "ngram_3_10": {
                    "max_gram": 10, 
                    "min_gram": 3, 
                    "type": "nGram"
                }, 
                "ngram_5_8": {
                    "max_gram": 8, 
                    "min_gram": 5, 
                    "type": "nGram"
                }, 
                "shingle_filter_2_4": {
                    "max_shingle_size": 4, 
                    "min_shingle_size": 2, 
                    "output_unigrams": false, 
                    "type": "shingle"
                }, 
                "shingle_filter_4": {
                    "max_shingle_size": 4, 
                    "min_shingle_size": 4, 
